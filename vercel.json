{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "cron-jobs.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/cron", "dest": "cron-jobs.js" },
    { "src": "/(.*)", "dest": "index.js" }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 * * * *"
    }
  ]
}
